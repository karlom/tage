{
  "name": "tage",
  "version": "1.0.0",
  "description": "Tage AI - AI Unified Desktop Client",
  "main": "dist-electron/electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:electron": "tsc -p tsconfig.node.json && tsc -p tsconfig.preload.json",
    "preview": "vite preview",
    "electron:dev": "npm run build:electron && concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "npm run build && npm run build:electron && electron-builder"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-plugin-electron": "^0.28.0",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/react-window": "^2.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "electron-store": "^11.0.2",
    "lucide-react": "^0.303.0",
    "mammoth": "^1.11.0",
    "pdf-parse": "^2.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "react-window": "^2.2.3",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^2.2.0"
  }
}
